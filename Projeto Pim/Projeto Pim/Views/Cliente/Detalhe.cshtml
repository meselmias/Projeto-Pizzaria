@model Projeto_Pim.Models.Cliente

@{
    Layout = null;
}
<!DOCTYPE html>

<html>
<head>
    <link href="~/Content/themes/base/bootstrap.min.css" rel="stylesheet" />
    <script src="~/Scripts/jquery.mask.min.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery-ui-1.8.20.js"></script>
    <script src="~/Scripts/jquery-ui-1.8.20.min.js"></script>
    <script src="~/Scripts/jquery-1.7.1.intellisense.js"></script>
    <script src="~/Scripts/jquery-1.7.1.js"></script>
    <script src="~/Scripts/jquery-1.7.1.min.js"></script>
    <script src="~/Scripts/jquery.validate-vsdoc.js"></script>
    <script src="~/Scripts/jquery.validate.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>
    <script src="~/Scripts/_references.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/knockout-2.1.0.debug.js"></script>
    <script src="~/Scripts/knockout-2.1.0.js"></script>
    <script src="~/Scripts/modernizr-2.5.3.js"></script>
    <title>detalhes</title>
</head>
     <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="navbar-inner">
            <div class="container">
                <button type="button" class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="brand" href="@Url.Action("Index", "Home")">Pizzaria Master</a>
                <div class="nav-collapse collapse">
                    <ul class="nav">
                        <li><a href="@Url.Action("Lista", "Cliente")">Cliente</a></li>
                        <li><a href="@Url.Action("Lista", "Pedido")">Pedido</a></li>
                    </ul>
                </div>
                <!--/.nav-collapse -->
            </div>
        </div>
    </div>
    <fieldset>
        <legend></legend>
        <div class="container">
            <div class="row">
                <div class="span4 input-prepend">
                    <span class="add-on">@Html.DisplayNameFor(model => model.Nome):</span>
                    @Html.TextBoxFor(model => model.Nome, new { @class = "input-medium uneditable-input", @readonly = "readonly" })
                </div>
                <div class="span4 input-prepend">
                    <span class="add-on">@Html.DisplayNameFor(model => model.Cpf):</span>
                    @Html.TextBoxFor(model => model.Cpf, new { @class = "input-medium uneditable-input", @readonly = "readonly" })
                </div>
                <div class="span4 input-prepend">
                    <span class="add-on">@Html.DisplayNameFor(model => model.DataNascimento):</span>
                    @Html.TextBoxFor(model => model.DataNascimento, new { @class = "input-medium uneditable-input", @readonly = "readonly" })
                </div>
            </div>
            <div class="row">
                <div class="span4 input-prepend">
                    <span class="add-on">@Html.DisplayNameFor(model => model.Endereco):</span>
                    @Html.TextBoxFor(model => model.Endereco, new { @class = "input-medium uneditable-input", @readonly = "readonly" })
                </div>
                <div class="span4 input-prepend">
                    <span class="add-on">@Html.DisplayNameFor(model => model.Cep):</span>
                    @Html.TextBoxFor(model => model.Cep, new { @class = "input-medium uneditable-input", @readonly = "readonly" })
                </div>
                <div class="span4 input-prepend">
                    <span class="add-on">@Html.DisplayNameFor(model => model.Complemento):</span>
                    @Html.TextBoxFor(model => model.Complemento, new { @class = "input-medium uneditable-input", @readonly = "readonly" })
                </div>
            </div>
            <div class="row">
                <div class="span4 input-prepend">
                    <span class="add-on">@Html.DisplayNameFor(model => model.Bairro):</span>
                    @Html.TextBoxFor(model => model.Bairro, new { @class = "input-medium uneditable-input", @readonly = "readonly" })
                </div>
                <div class="span4 input-prepend">
                    <span class="add-on">@Html.DisplayNameFor(model => model.PontoReferencia):</span>
                    @Html.TextBoxFor(model => model.PontoReferencia, new { @class = "input-medium uneditable-input", @readonly = "readonly" })
                </div>
                <div class="span4 input-prepend">
                    <span class="add-on">@Html.DisplayNameFor(model => model.Email):</span>
                    @Html.TextBoxFor(model => model.Email, new { @class = "input-medium uneditable-input", @readonly = "readonly" })
                </div>
            </div>
            <div class="row">
                <div class="span4 input-prepend">
                    <span class="add-on">@Html.DisplayNameFor(model => model.Telefone):</span>
                    @Html.TextBoxFor(model => model.Telefone, new { @class = "input-medium uneditable-input", @readonly = "readonly" })
                </div>
            </div>
        </div>
        <div class="row">
            <div class="span4 offset3">
                <a href="@Url.Action("Lista", "Cliente")" class="btn btn-small btn-primary"><i class="icon-list icon-white"></i>lista de Clientes</a>
            </div>
        </div>

    </fieldset>

</html>




